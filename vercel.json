{
  "version": 2,
  "functions": {
    "api/server.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/api/server.js"
    }
  ]
}